# OpenRouter API key — required for all LLM calls (chat agent + title generation).
OPENROUTER_API_KEY=

# Supabase local auth provider secrets (used by `supabase start`)
# Required only if testing Google OAuth login flow.
# Leave empty or remove if using headless test mode.
SUPABASE_AUTH_EXTERNAL_GOOGLE_CLIENT_ID=
SUPABASE_AUTH_EXTERNAL_GOOGLE_CLIENT_SECRET=
SUPABASE_SERVICE_ROLE_KEY=


NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=
NEXT_PUBLIC_SITE_URL=

# Google OAuth credentials for Drive integration.
# Can reuse the same OAuth client ID/secret as Supabase Auth above —
# just add http://127.0.0.1:3000/auth/integrations/google-drive/callback
# as an authorized redirect URI in Google Cloud Console.
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
GOOGLE_REDIRECT_URI=http://127.0.0.1:3000/auth/integrations/google-drive/callback

# 32-byte hex key for AES-256-GCM encryption of stored refresh tokens.
# Generate with: openssl rand -hex 32
TOKEN_ENCRYPTION_KEY=